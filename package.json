{
  "name": "lyricistant",
  "productName": "Lyricistant",
  "displayName": "Lyricistant",
  "version": "1.0.0",
  "license": "GPL-3.0-or-later",
  "description": "An app that helps you to write the lyrics you want to!",
  "author": {
    "name": "Wardell Bagby",
    "email": "dev@wardellbagby.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/wardellbagby/lyricistant.git"
  },
  "homepage": "https://github.com/wardellbagby/lyricistant",
  "sideEffects": [
    "node_modules/codemirror"
  ],
  "scripts": {
    "build-app": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" webpack --config config/electron/prod/webpack.config.ts",
    "build-web": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" webpack --config config/web/prod.webpack.config.ts",
    "lint": "tslint -p tsconfig.json",
    "lint-fix": "tslint -p tsconfig.json --fix",
    "start-web": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" webpack-dev-server --open --config config/web/dev.webpack.config.ts",
    "start-app": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" concurrently --names \"Renderer,Main\" \"webpack-dev-server --config config/electron/dev/renderer.webpack.config.ts\" \"webpack --watch --config config/electron/dev/main.webpack.config.ts\"",
    "dist-app": "electron-builder --config electron-builder.json --publish never",
    "dist-all-apps": "electron-builder -mwl --config electron-builder.json --publish never",
    "test": "ts-mocha -r tsconfig-paths/register -p config/tsconfig-for-webpack-config.json",
    "build-test-app": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" webpack --config config/electron/test/webpack.config.ts",
    "dist-test-app": "cross-env electron-builder --mac --publish never -c.mac.identity=null",
    "build-test-web": "cross-env TS_NODE_PROJECT=\"config/tsconfig-for-webpack-config.json\" webpack --config config/web/test.webpack.config.ts"
  },
  "engines": {
    "node": ">= 12.10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix && ts-node --project config/tsconfig-for-webpack-config.json bin/check_github_workflows.ts"
    }
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/chai-subset": "^1.3.3",
    "@types/circular-dependency-plugin": "^5.0.1",
    "@types/codemirror": "0.0.98",
    "@types/compression-webpack-plugin": "^4.0.1",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/luxon": "^1.25.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.54",
    "@types/prettier": "^2.0.2",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^16.9.45",
    "@types/react-beforeunload": "^2.1.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/sinon": "^9.0.5",
    "@types/sprintf-js": "^1.1.2",
    "@types/terser-webpack-plugin": "^3.0.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
